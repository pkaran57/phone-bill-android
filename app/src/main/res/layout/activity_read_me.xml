<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".app.activity.ReadMeActivity">

    <TextView
            android:text="Read Me"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView5"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.451"
            app:layout_constraintVertical_bias="0.042"/>
    <TextView
            android:text="@string/read_me_text"
            android:layout_width="374dp"
            android:layout_height="601dp" app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/read_me" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginTop="32dp"
            app:layout_constraintHorizontal_bias="0.372"/>

</androidx.constraintlayout.widget.ConstraintLayout>
