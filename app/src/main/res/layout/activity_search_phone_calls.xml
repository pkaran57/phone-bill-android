<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".app.activity.SearchPhoneCallsActivity">

    <EditText
            android:layout_width="258dp"
            android:layout_height="52dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/end_edit_text_2"
            android:hint="02/27/2020 10:27 am"
            app:layout_constraintTop_toBottomOf="@+id/start_edit_text_2"
            android:layout_marginTop="12dp" app:layout_constraintStart_toEndOf="@+id/textView11"
            android:layout_marginStart="22dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="260dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/start_edit_text_2"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintTop_toBottomOf="@+id/phone_bill_customer_name_edit_text2"
            android:hint="02/27/2020 8:56 am" android:layout_marginTop="20dp"
            app:layout_constraintStart_toEndOf="@+id/textView12" app:layout_constraintHorizontal_bias="0.909"/>
    <TextView
            android:text="End"
            android:layout_width="96dp"
            android:gravity="center"
            android:layout_height="47dp" android:id="@+id/textView11"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            app:layout_constraintEnd_toStartOf="@+id/end_edit_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.28"
            app:layout_constraintVertical_bias="0.148" app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintBottom_toTopOf="@+id/add_phone_call_to_phone_bill_button"
            android:layout_marginTop="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Start"
            android:layout_width="97dp"
            android:gravity="center"
            android:layout_height="44dp" android:id="@+id/textView12"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            app:layout_constraintEnd_toStartOf="@+id/start_edit_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.298"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            android:layout_marginTop="20dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Customer Name"
            android:layout_width="151dp"
            android:gravity="center"
            android:layout_height="78dp" android:id="@+id/textView8"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/phone_bill_customer_name_edit_text2"
            android:layout_marginTop="16dp" app:layout_constraintHorizontal_bias="0.526"/>
    <EditText
            android:layout_width="226dp"
            android:layout_height="78dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/phone_bill_customer_name_edit_text2"
            app:layout_constraintEnd_toEndOf="parent"
            android:hint="Customer Name"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginTop="16dp"/>
    <Button
            android:text="Search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/search_phone_calls_button_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintTop_toBottomOf="@+id/search_phone_calls_result_list_view"
            android:layout_marginTop="16dp"/>
    <ListView
            android:layout_width="373dp"
            android:layout_height="291dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/search_phone_calls_result_list_view"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.605" app:layout_constraintTop_toBottomOf="@+id/end_edit_text_2"
            app:layout_constraintVertical_bias="0.127"/>
</androidx.constraintlayout.widget.ConstraintLayout>